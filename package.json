{
  "name": "act",
  "version": "0.0.2",
  "description": "React Native for Web with Actheme and Actstore",
  "main": "index.js",
  "scripts": {
    "start": "ENV=test next -p 8080",
    "start:win": "set ENV=test&& next -p 8080",
    "build": "next build",
    "export": "next export",
    "serve": "serve ./out -p 8080",
    "static": "npm run build && npm run export && touch ./out/.nojekyll && npm run serve",
    "deploy": "git subtree push --prefix out origin gh-pages"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "actheme": "0.0.30",
    "actstore": "0.0.32",
    "fetchier": "0.0.30",
    "next": "^9.4.4",
    "pack": "file:./src/@pack",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native-web": "^0.12.2"
  },
  "devDependencies": {
    "babel-plugin-react-native-web": "^0.12.2",
    "next-transpile-modules": "^3.2.0",
    "serve": "^11.3.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "react-native-web",
        {
          "commonjs": true
        }
      ]
    ]
  }
}
